
<div>
  <h1 style="text-transform: capitalize;">Welcome, {{username}}!</h1>
  <div class="row">
    <div class="col-4">
      <p>Your printers:</p>
      {{#each printers as |printer|}}
      <div>
        {{printer.name}} - {{printer.manufacturer}} - {{printer.model}}
      </div>
      {{/each}}
      <button id="btnAddPrinter">Add Printer</button>
    </div>
    <div class="col-6">
      <p>Your filaments:</p>
      {{#each filaments as |filament|}} 
      <div>
        {{filament.manufacturer}} - {{filament.color}} - {{filament.diameter}}
      </div>
      {{/each}}
      <button id="btnAddFilament">Add Filament</button>
    </div>
    <div class="col-2">
      <button id="btnSettings">Printer Settings</button>
    </div>
  </div>

{{#if logged_in}}
<script src="/js/profile.js"></script> 
{{/if}}